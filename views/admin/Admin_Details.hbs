

<section>
    <div class="container">
        <div class="row">
            <h2>Admins</h2>
            <div>
                <table class="table mt-5">
  <thead class="table-dark">
    <tr>
      <th scope="col">S.No</th>
      <th scope="col">Name</th>
      <th scope="col">Department</th>
      <th scope="col">Email</th>
      <th scope="col"></th>
      
      
    </tr>
  </thead>
  <tbody>
     {{!-- {{#each users}} --}}
     {{#each admins}}
    <tr>
      <th scope="row">></th>
      <td>{{this.Name}}</td>
      <td>{{this.Department}}</td>
      <td>{{this.Email}}</td>
      <td><a href="/admin/delete-admin/{{this._id}}" class="btn btn-danger" onclick="return confirm('Are you what to Delete {{this.Name}}')">Delete</a></td>

     
    </tr>
   
    {{/each}}
    
  </tbody>
</table>
            </div>
        </div>
    </div>
</section>